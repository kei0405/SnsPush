<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd
">

   <bean id="topicArn" class="java.lang.String" >
      <constructor-arg value="arn:aws:sns:ap-northeast-1:213666321898:MyNewTopic" />
   </bean>
   
   <bean id="appArn" class="java.lang.String" >
      <constructor-arg value="arn:aws:sns:ap-northeast-1:213666321898:app/APNS_SANDBOX/PushApp"/>
   </bean>
</beans>